<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <exclude domain="cache" />
        <!-- Exclude Room database files from cloud backup -->
        <exclude domain="database" path="note_database" />
        <exclude domain="database" path="note_database-shm" />
        <exclude domain="database" path="note_database-wal" />
    </cloud-backup>
    <device-transfer>
        <exclude domain="cache" />
        <!-- Exclude Room database from device-to-device transfer -->
        <exclude domain="database" path="note_database" />
        <exclude domain="database" path="note_database-shm" />
        <exclude domain="database" path="note_database-wal" />
    </device-transfer>
</data-extraction-rules> 